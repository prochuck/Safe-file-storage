<Window x:Class="Safe_file_storage.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Safe_file_storage"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ListBox x:Name="listBox" ItemsSource="{Binding}" Margin="0,10,0,124"/>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="355,374,0,0" VerticalAlignment="Top" Click="button_Click"/>
    </Grid>
</Window>
